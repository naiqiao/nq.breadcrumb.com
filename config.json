{
	"configVersion": "1",
	"deviceDetected": false,
	"https": false,
	"liveSite": false,
	"locales": ["en","es","pt"],
	"templateVersion": 2,
	"features": [
		{
			"name": "Locations",
			"locationPageSet": {
				"urlTemplate": "url.location",
				"template": "desktop.location.default",
				"outputFormat": ["Html", "Json"]
			}
		},
		{
			"name": "Location Directory",
			"locationDirectory": {
				"levels": [
					 "levels": [
          				{
					    "urlTemplate": "url.dir_808_country",
					    "template": "desktop.directory.countryList",
					    "field": "Country",
					    "orderBy": {
					      "field": "Country",
					      "order": "ascending"
					    },
					    "limit": 0
					},
					{
						"urlTemplate": "url.dir_808_state",
						"template": "desktop.directory.stateList",
						"field": "State",
						"orderBy": {
							"field": "State",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "url.dir_808_city",
						"template": "desktop.directory.cityList",
						"field": "City",
						"orderBy": {
							"field": "City",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "url.dir_808_locations",
						"template": "desktop.directory.locationList",
						"field": "Locations",
						"orderBy": {
							"field": "Locations",
							"order": "ascending"
						}
					}
				],
				"locationPageSet": "Locations",
				"outputFormat": ["Html", "Json"]
			}
		}
	]
}
